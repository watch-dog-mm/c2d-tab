{
  "name": "Your daily C2D",
  "description": "အရေးတော်ပုံအောင်ရမည်",
  "manifest_version": 3,
  "version": "1.0.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": ["storage",  "tabs","activeTab","scripting"],
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png"
    },
    "default_popup": "/index.html#/setting"
  },
  
  "host_permissions": ["<all_urls>"],
  "background": {
    
    "service_worker": "background.js"

  }
}